        "#----------------------------------- 2.2 방법1. 이미지 더해주기\r\n",
        "\r\n",
        "#========================================== 3. Title 적어주기\r\n",
        "figsize = (15,5)\r\n",
        "facecolor = 'grey'\r\n",
        "fig =  plt.figure(figsize=figsize,\r\n",
        "                  facecolor=facecolor)\r\n",
        "#colors = ['skyblue','palegreen','pink']\r\n",
        "titles = ['Graph' + str(i) for i in range(1,4,1)]\r\n",
        "axes = []\r\n",
        "for ax_idx, ax in enumerate(colors) :    \r\n",
        "    ax = fig.add_subplot(1,3,int(ax_idx+1),\r\n",
        "                         facecolor=colors[ax_idx])\r\n",
        "    ax.set_title(titles[ax_idx])\r\n",
        "    axes.append(ax)"
