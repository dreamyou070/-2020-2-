        "id": "Kx84hvs2dvS-",
        "outputId": "805fd710-e043-494c-ab3c-53578764c55f"
      },
      "source": [
        "#========================================= 1. Package Import \r\n",
        "import matplotlib.pyplot as  plt\r\n",
        "import numpy as np\r\n",
        "\r\n",
        "#========================================== 2. 전체적인 이미지 윤곽 잡아주기\r\n",
        "figsize = (15,5)\r\n",
        "facecolor = 'grey'\r\n",
        "fig =  plt.figure(figsize=figsize,\r\n",
        "                  facecolor=facecolor)\r\n",
        "#----------------------------------- 2.3 방법3. 이미지 더해주기 : 세부 위치 잡기 (subplot2grid)\r\n",
        "#-------------------------- 1번 그래프\r\n",
        "ax1 = plt.subplot2grid(shape=(3,4),\r\n",
        "                       loc=(0,0),\r\n",
        "                       colspan=2, \r\n",
        "                       fig=fig)\r\n",
        "#-------------------------- 2번 그래프\r\n",
        "ax2 = plt.subplot2grid(shape=(3,4),\r\n",
        "                       loc=(0,2),\r\n",
        "                       colspan=2,\r\n",
        "                       rowspan=3,\r\n",
        "                       fig=fig)\r\n",
        "#-------------------------- 3번 그래프\r\n",
        "ax3 = plt.subplot2grid(shape=(3,4),\r\n",
        "                       loc=(1,0),                       \r\n",
        "                       rowspan=2,\r\n",
        "                       fig=fig)\r\n",
        "#-------------------------- 4번 그래프\r\n",
        "ax4 = plt.subplot2grid(shape=(3,4),\r\n",
        "                       loc=(1,1),                       \r\n",
        "                       fig=fig)\r\n",
        "#-------------------------- 5번 그래프\r\n",
        "ax5 = plt.subplot2grid(shape=(3,4),\r\n",
        "                       loc=(2,1),                       \r\n",
        "                       fig=fig)"
      ],
      "execution_count": null,
      "outputs": [
